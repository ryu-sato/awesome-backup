{
  "name": "awesome-backup",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.31",
    "@types/rewire": "^2.5.28",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "babel-eslint": "^10.1.0",
    "commander": "^9.2.0",
    "eslint": "^8.9.0",
    "eslint-config-weseek": "^1.1.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.5",
    "jest": "^27.4.5",
    "rewire": "^6.0.0",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "tsc-alias": "^1.5.0",
    "turbo": "^1.2.5",
    "typescript": "^4.6.4",
    "rimraf": "^3.0.2",
    "@changesets/cli": "^2.22.0"
  },
  "scripts": {
    "build": "turbo run build",
    "test": "turbo run test --concurrency='100%'",
    "rimraf": "rimraf",
    "clean": "npm run rimraf node_modules/.cache .turbo apps/*/dist packages/*/dist */*/coverage */*/.turbo"
  },
  "packageManager": "yarn@1.22.15",
  "turbo": {
    "pipeline": {
      "build": {
        "dependsOn": ["^build"],
        "outputs": ["dist/**"]
      },
      "test": {
        "// Test depends on build because 'rewire' read transpiled file.": "",
        "dependsOn": ["^build", "build"],
        "outputs": ["coverage/**"]
      },
      "lint": {
        "outputs": []
      }
    }
  }
}
